# 设置固件大小:
CONFIG_TARGET_ramips=y
CONFIG_TARGET_ramips_mt76x8=y
CONFIG_TARGET_ramips_mt76x8_DEVICE_hiwifi_hc5861b=y

CONFIG_PACKAGE_luci-app-turboacc=n
CONFIG_PACKAGE_luci-app-turboacc_INCLUDE_flow-offload=n
CONFIG_PACKAGE_luci-app-turboacc_INCLUDE_shortcut-fe=n

CONFIG_KERNEL_MIPS_FPU_EMULATOR=y

CONFIG_PACKAGE_luci-app-bypass=n
CONFIG_PACKAGE_luci-app-bypass_INCLUDE_V2ray=n
CONFIG_PACKAGE_luci-app-bypass_INCLUDE_Xray=n
CONFIG_PACKAGE_luci-app-bypass_INCLUDE_Trojan=n
CONFIG_PACKAGE_luci-app-bypass_INCLUDE_Trojan-Go=n
CONFIG_PACKAGE_luci-app-bypass_INCLUDE_Simple_obfs=n
CONFIG_PACKAGE_luci-app-guest-wifi=n
CONFIG_PACKAGE_luci-app-ramfree=y

# 其他需要安装的软件包:
CONFIG_PACKAGE_autocore-switch=y
CONFIG_V2RAY_COMPRESS_UPX=y
CONFIG_XRAY_COMPRESS_UPX=y
CONFIG_PACKAGE_bash=n
CONFIG_PACKAGE_htop=n
CONFIG_PACKAGE_nano=n
CONFIG_PACKAGE_wget-ssl=n

# cifsmount
CONFIG_PACKAGE_kmod-fs-cifs=n
CONFIG_PACKAGE_kmod-nls-utf8=n
CONFIG_PACKAGE_kmod-crypto-misc=n

# wireguard
CONFIG_PACKAGE_kmod-wireguard=n

# Usb support
CONFIG_PACKAGE_kmod-usb-core=y
CONFIG_PACKAGE_kmod-usb-ehci=y
CONFIG_PACKAGE_kmod-usb-ohci=y
CONFIG_PACKAGE_kmod-usb-printer=y
CONFIG_PACKAGE_kmod-usb-storage=y
CONFIG_PACKAGE_kmod-usb-storage-extras=y
CONFIG_PACKAGE_kmod-usb-uhci=y
CONFIG_PACKAGE_kmod-usb2=y
CONFIG_PACKAGE_kmod-scsi-core=y
# Filesystems
CONFIG_PACKAGE_kmod-fs-ext4=y
CONFIG_PACKAGE_kmod-fs-ntfs=y
CONFIG_PACKAGE_kmod-fs-vfat=y
CONFIG_PACKAGE_kmod-fuse=y
# Native Language Support
CONFIG_PACKAGE_kmod-nls-base=y
CONFIG_PACKAGE_kmod-nls-cp437=y
CONFIG_PACKAGE_kmod-nls-iso8859-1=y
CONFIG_PACKAGE_kmod-nls-utf8=y
# Utilities
CONFIG_PACKAGE_fdisk=y
CONFIG_PACKAGE_libfdisk=y
CONFIG_PACKAGE_usbutils=y

